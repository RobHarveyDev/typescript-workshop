{
  "name": "web-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.8",
    "@mui/icons-material": "^5.14.14",
    "@mui/material": "^5.14.13",
    "@mui/x-date-pickers": "^6.16.3",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "express": "^4.18.2",
    "http-status-codes": "^2.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "recharts": "^2.9.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "backend": "ts-node-dev --respawn --pretty --transpile-only src/api/index.ts",
    "frontend": "react-scripts start",
    "test:frontend": "react-scripts test --testPathPattern=tests/frontend",
    "test:api": "react-scripts test --testPathPattern=tests/backend --coverage --collectCoverageFrom=src/api",
    "workshop:1": "react-scripts test --coverage --collectCoverageFrom=src/api/repositories/pet-repository.ts tests/backend/repositories/pet-repository.test.ts",
    "workshop:2": "react-scripts test tests/backend/services/pet-service.test.ts",
    "workshop:3": "react-scripts test --testPathPattern=tests/frontend"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.19",
    "@types/jest": "^29.5.10",
    "@types/jquery": "^3.5.20",
    "@types/node": "^20.8.6",
    "@types/react": "^18.2.24",
    "@types/react-dom": "^18.2.8",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tslib": "^2.6.2",
    "typescript": "^4.9.5"
  }
}
