{
  "compilerOptions": {
    "lib": ["ES2016", "DOM"],
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "./dist",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "strict": true,
    "noImplicitAny": true,
    "downlevelIteration": true,
    "resolveJsonModule": true,
    "esModuleInterop": true
  }
}
